<%- include("header") %>

<div class="box" id="heading">
  <h1><%=title %></h1>
</div>



<div class="box">

  <% myItems.forEach( (item) => { %>
    <div class="item">
      <input type = "checkbox">
      <p><%= item.item %> </p>
    </div>
  <% }) %>


    <form action="/" method="POST" class="item">
      <input
        type="text"
        name="addItem"
        placeholder="New Item"
        autocomplete="off"
      />
      <button type="submit" name="listName" value=<%=title%> >+</button>
    </form>
  
</div>


<%- include("footer") %>
